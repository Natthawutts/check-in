<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
</head>
<body>
    <table>
        <thead>
            <tr>
                <th>ID</th>
                <th>First Name</th>
                <th>Last Name</th>
                <th>Date of Birth</th>
                <th>Sex</th>
                <th>Status</th>
            </tr>
        </thead>
        <tbody>
            <% students.forEach(student => { %>
            <tr>
                <td><%= student.id %></td>
                <td><%= student.first_name %></td>
                <td><%= student.last_name %></td>
                <td><%= student.date_of_birth %></td>
                <td><%= student.sex %></td>
                <td><%= student.status %></td>
            </tr>
            <% }) %>
        </tbody>
    </table>
</body>
</html>